<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>


<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
	prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
	<top>
		<HBox spacing="10.0" BorderPane.alignment="CENTER_LEFT">
			<children>
				<Label text="Form Absensi" />
			</children>
			<padding>
				<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
			</padding>
		</HBox>
	</top>
	<center>
		<AnchorPane BorderPane.alignment="CENTER">
			<children>
				<TableView fx:id="tableView" prefHeight="364.0" prefWidth="600.0"
					AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
					AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
					<columns>
						<TableColumn fx:id="columnNik" maxWidth="120.0"
							minWidth="120.0" prefWidth="120.0" text="NIK" />
						<TableColumn fx:id="columnNama" editable="false"
							maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="-1.0"
							sortable="false" text="Nama" />
						<TableColumn fx:id="columnAbsen" editable="false"
							maxWidth="100.0" minWidth="200.0" prefWidth="200.0" sortable="false"
							text="Absen" />
					</columns>
					<columnResizePolicy>
						<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
					</columnResizePolicy>
				</TableView>
			</children>
			<padding>
				<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
			</padding>
		</AnchorPane>
	</center>
	<bottom>
		<ToolBar prefHeight="40.0" prefWidth="200.0"
			BorderPane.alignment="CENTER">
			<items>
				<Button fx:id="btnTambah" onAction="#doSave" cancelButton="true"
					defaultButton="true" mnemonicParsing="false" prefHeight="30.0"
					prefWidth="100.0" text="Tambah" />
				<Button onAction="#doCancel" cancelButton="true" layoutX="10.0"
					layoutY="10.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="100.0"
					text="Batal" />
			</items>
		</ToolBar>
	</bottom>
</BorderPane>
