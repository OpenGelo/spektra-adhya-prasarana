<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
	prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
	<top>
		<HBox alignment="CENTER_LEFT" spacing="10.0"
			BorderPane.alignment="CENTER">
			<children>
				<Label text="Data jabatan">
					<font>
						<Font name="System Bold" size="16.0" />
					</font>
				</Label>
			</children>
			<padding>
				<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
			</padding>
		</HBox>
	</top>
	<bottom>
		<ToolBar prefHeight="40.0" prefWidth="200.0"
			BorderPane.alignment="CENTER">
			<items>
				<Button cancelButton="true" defaultButton="true"
					mnemonicParsing="false" onAction="#doSave" prefHeight="30.0"
					prefWidth="100.0" text="Simpan" />
				<Button cancelButton="true" layoutX="10.0" layoutY="10.0"
					mnemonicParsing="false" onAction="#doCancel" prefHeight="30.0"
					prefWidth="100.0" text="Batal" />
			</items>
		</ToolBar>
	</bottom>
	<center>
		<ScrollPane fitToHeight="true" fitToWidth="true"
