<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
	prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
	<top>
		<HBox alignment="CENTER_LEFT" spacing="10.0"
			style="-fx-background-color: GRAY;" BorderPane.alignment="CENTER">
			<children>
				<Label text="Pengajuan kasbon karyawan" textFill="WHITE">
					<font>
						<Font name="System Bold" size="25.0" />
					</font>
				</Label>
			</children>
			<padding>
				<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
			</padding>
		</HBox>
	</top>
	<center>
		<ScrollPane fitToHeight="true" fitToWidth="true"
			hbarPolicy="NEVER" vbarPolicy="ALWAYS" BorderPane.alignment="CENTER">
			<content>
				<GridPane hgap="10.0" vgap="10.0">
					<columnConstraints>
						<ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity"
							minWidth="-Infinity" />
						<ColumnConstraints hgrow="SOMETIMES" maxWidth="150.0"
							minWidth="150.0" prefWidth="150.0" />
						<ColumnConstraints hgrow="SOMETIMES" maxWidth="120.0"
							minWidth="120.0" prefWidth="120.0" />
					</columnConstraints>
					<rowConstraints>
						<RowConstraints maxHeight="30.0" minHeight="30.0"
							prefHeight="30.0" vgrow="SOMETIMES" />
						<RowConstraints maxHeight="30.0" minHeight="30.0"
							prefHeight="30.0" vgrow="SOMETIMES" />
						<RowConstraints maxHeight="30.0" minHeight="30.0"
							prefHeight="30.0" vgrow="SOMETIMES" />
						<RowConstraints maxHeight="30.0" minHeight="30.0"
							prefHeight="30.0" vgrow="SOMETIMES" />
					</rowConstraints>
					<padding>
						<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
					</padding>
					<children>
						<AnchorPane GridPane.columnIndex="1">
							<children>
								<DatePicker fx:id="txtTanggalTransaksi" prefHeight="30.0"
									prefWidth="198.0" promptText="Tanggal Transaksi"
									AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
									AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
							</children>
						</AnchorPane>
						<AnchorPane GridPane.columnIndex="1"
							GridPane.columnSpan="2" GridPane.rowIndex="2">
							<children>
								<Spinner fx:id="txtPinjam" disable="true" editable="true"
									prefHeight="30.0" prefWidth="175.0" AnchorPane.bottomAnchor="0.0"
									AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
									AnchorPane.topAnchor="0.0" />
							</children>
						</AnchorPane>
						<Label text="Tanggal" />
						<Label text="Karyawan" GridPane.rowIndex="1" />
						<Text strokeType="OUTSIDE" strokeWidth="0.0" text="Jumlah Pinjam"
							GridPane.rowIndex="2" />
						<AnchorPane GridPane.columnIndex="1"
							GridPane.columnSpan="2" GridPane.rowIndex="3">
							<children>
								<TextField fx:id="txtNominal" alignment="CENTER_RIGHT"
									editable="false" focusTraversable="false" prefHeight="30.0"
									prefWidth="200.0" promptText="Rp.0,00" AnchorPane.bottomAnchor="0.0"
									AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
									AnchorPane.topAnchor="0.0" />
							</children>
						</AnchorPane>
						<AnchorPane GridPane.columnIndex="1"
							GridPane.columnSpan="2" GridPane.rowIndex="1">
							<children>
								<ComboBox prefHeight="30.0" prefWidth="150.0"
									promptText="Pilih Karyawan" AnchorPane.bottomAnchor="0.0"
									AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
									AnchorPane.topAnchor="0.0" />
							</children>
						</AnchorPane>
					</children>
				</GridPane>
			</content>
		</ScrollPane>
	</center>
	<bottom>
		<ToolBar prefHeight="40.0" prefWidth="200.0"
			BorderPane.alignment="CENTER">
			<items>
				<Button fx:id="btnSimpan" defaultButton="true" disable="true"
					mnemonicParsing="false" prefHeight="30.0" prefWidth="100.0" text="Simpan" />
				<Button cancelButton="true" layoutX="10.0" layoutY="10.0"
					mnemonicParsing="false" onAction="#doBack" prefHeight="30.0"
					prefWidth="100.0" text="Batal" />
			</items>
		</ToolBar>
	</bottom>
</BorderPane>
